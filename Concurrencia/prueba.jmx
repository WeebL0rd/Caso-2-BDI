        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Petición JDBC">
          <stringProp name="dataSource">soltura</stringProp>
          <stringProp name="queryType">Update Statement</stringProp>
          <stringProp name="query">;WITH toProcess AS (
  SELECT TOP (50) queueID, userID, amount, payload
  FROM solturaDB.sol_payment_queue
  ORDER BY queueID
)
-- 1) Batch INSERT en la tabla real
INSERT INTO solturaDB.sol_payments
  (availableMethodID, currency_id, amount, date_pay, confirmed,
   result, auth, reference, charge_token, description, checksum, methodID)
SELECT
  1,1, tp.amount, GETDATE(), 1,
  &apos;Aprobado&apos;,
  JSON_VALUE(tp.payload,&apos;$.ref&apos;),
  CAST(NEWID() AS VARCHAR(50)),
  0x00,
  &apos;Batch desde cola&apos;,
  0x00,
  1
FROM toProcess tp;

-- 2) Elimina los procesados de la cola
DELETE q
FROM solturaDB.sol_payment_queue q
JOIN toProcess tp ON q.queueID = tp.queueID;
</stringProp>
